{
  "extends": "./tsconfig.json",
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "compilerOptions": {
    "types": ["node", "jest"]
  },
  "include": ["src/**/*.spec.ts",
    "test/**/*.ts"
  ]
}
